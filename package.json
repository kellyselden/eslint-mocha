{
  "name": "eslint-mocha",
  "version": "0.0.10",
  "description": "Auto inject ESLint tests into your Mocha suite",
  "main": "dist/eslint-mocha.js",
  "bin": {
    "eslint-mocha": "bin/eslint-mocha.js"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel lib -d dist",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "precover": "npm run build",
    "test": "mocha --compilers js:babel-register \"test/!(fixtures)/**/*-test.js\" test/lint.js assets/test-file.js",
    "cover": "babel-node node_modules/isparta/bin/isparta cover --include-all-sources --report html --report lcov node_modules/mocha/bin/_mocha -- --compilers js:babel-register \"test/!(fixtures)/**/*-test.js\" test/lint.js assets/test-file.js",
    "debug": "node debug node_modules/mocha/bin/_mocha --no-timeouts --compilers js:babel-register \"test/unit/**/*-test.js\"",
    "lint": "mocha test/lint.js assets/test-file.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kellyselden/eslint-mocha.git"
  },
  "author": "Kelly Selden <kellyselden@gmail.com> (https://github.com/kellyselden)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kellyselden/eslint-mocha/issues"
  },
  "homepage": "https://github.com/kellyselden/eslint-mocha#readme",
  "dependencies": {
    "eol": "0.2.0",
    "lodash": "^4.0.0",
    "yargs": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-plugin-rewire": "1.0.0-rc-3",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-register": "^6.0.0",
    "chai": "^3.0.0",
    "coveralls": "^2.0.0",
    "eslint": "^2.0.0",
    "isparta": "^4.0.0",
    "mocha": "^2.0.0",
    "rimraf": "^2.0.0",
    "sinon": "^1.0.0"
  },
  "peerDependencies": {
    "eslint": ">=3.1.0",
    "mocha": ">=1 <3"
  },
  "keywords": [
    "eslint",
    "mocha"
  ],
  "engines": {
    "node": ">= 0.12.0"
  }
}
