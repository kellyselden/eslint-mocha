{
  "name": "eslint-mocha",
  "version": "0.0.4",
  "description": "Auto inject ESLint tests into your Mocha suite",
  "main": "dist/eslint-mocha.js",
  "preferGlobal": true,
  "bin": {
    "eslint-mocha": "bin/eslint-mocha.js"
  },
  "scripts": {
    "build": "babel lib -d dist",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "precover": "npm run build",
    "test": "mocha --compilers js:babel-register \"test/@(acceptance|unit)/**/*-test.js\" test/lint.js assets/test-file.js",
    "cover": "babel-node node_modules/isparta/bin/isparta cover --include-all-sources --report html --report lcov node_modules/mocha/bin/_mocha -- --compilers js:babel-register \"test/@(acceptance|unit)/**/*-test.js\" test/lint.js assets/test-file.js",
    "debug": "node debug node_modules/mocha/bin/_mocha --no-timeouts --compilers js:babel-register \"test/unit/**/*-test.js\"",
    "lint": "mocha test/lint.js assets/test-file.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kellyselden/eslint-mocha.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kellyselden/eslint-mocha/issues"
  },
  "homepage": "https://github.com/kellyselden/eslint-mocha#readme",
  "dependencies": {
    "eol": "0.2.0",
    "eslint": "^1.0.0",
    "lodash": "^3.0.0",
    "mocha": "^2.0.0",
    "yargs": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-plugin-rewire": "1.0.0-beta-3",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-register": "^6.0.0",
    "chai": "^3.0.0",
    "coveralls": "^2.0.0",
    "isparta": "^4.0.0",
    "sinon": "^1.0.0"
  }
}
